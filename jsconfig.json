{
  "compilerOptions": {
    "jsx": "react-jsx",
    "checkJs": true,
    "baseUrl": "./",
    "types": ["vitest/globals", "react"],
    "paths": {
      "@images/*": ["src/resources/images/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@reducers/*": ["src/reducers/*"],
      "@pages/*": ["src/pages/*"],
      "@providers/*": ["src/providers/*"],
      "@/*": ["src/*"]
    }
  },
  "updateImportsOnFileMove": "prompt"
}
